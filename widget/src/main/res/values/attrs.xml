<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="AvatarView">
		<attr name="avBorderColor" format="color|reference" />
		<attr name="avBorderColorSecondary" format="color|reference" />
		<attr name="avBorderGradientAngle" format="integer" />
		<attr name="avBackgroundColor" format="color|reference" />
		<attr name="avBorderWidth" format="dimension" />
		<attr name="avTextSizePercentage" format="float" />
		<attr name="avVolumetricType" format="enum" >
			<enum name="none" value="-1"/>
			<enum name="all" value="0"/>
			<enum name="drawable" value="1"/>
			<enum name="placeholder" value="2"/>
		</attr>
		<attr name="avAvatarMargin" format="dimension"/>
		<attr name="avArchesDegreeArea" format="integer"/>
		<attr name="avArchesAngle" format="integer"/>
		<attr name="avArchesCount" format="integer"/>
		<attr name="android:fontFamily" />
		<attr name="iconDrawableScale" format="float"/>
		<attr name="placeholderText" format="string"/>
		<attr name="android:src"/>
	</declare-styleable>

</resources>